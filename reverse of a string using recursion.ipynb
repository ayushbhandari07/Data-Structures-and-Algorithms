   "source": [
    "# find the reverse of a string using recursion"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "def print_rev(s):\n",
    "    if len(s)==1:\n",
    "        return s\n",
    "    else:\n",
    "        return print_rev(s[1:])+s[0]"
   ]
  },
